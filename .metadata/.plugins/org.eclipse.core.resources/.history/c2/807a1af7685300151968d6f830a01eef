import java.net.URI;

import javax.swing.JButton;
import javax.swing.JFrame;
import javax.swing.JPanel;


public class CütnišTV {
JButton thefirstbutton = new JButton();
JButton thesecondbutton = new JButton();
JButton thethirdbutton = new JButton();
	public static void main(String[] args) {
		JFrame frame1 = new JFrame();
		JPanel pOne = new JPanel();
		frame1.add(pOne);
		thefirstbutton = this.createButton("You definitely should click here because I spelled all these words correctly!");
		
	}
	
	void showDucks() {
		playVideo("EpiGS-3PDU8");
	}

	void showFrog() {
		playVideo("aSjCb-FfxhI");
	}
	
	void showFluffyUnicorns() {
		playVideo("qRC4Vk6kisY");
	}

	void playVideo(String videoID) {
		try {
	URI uri = new URI("https://www.youtube.com/v/" + videoID + "?autoplay=1");
			java.awt.Desktop.getDesktop().browse(uri);
		} catch (Exception e) {
			e.printStackTrace();
		}
	}



}
